  "description": "My Electron application description",
  "main": ".webpack/main",
  "scripts": {
    "prestart": "node -e \"if(process.platform !== 'win32') { require('child_process').exec('lsof -ti:5328 | xargs -I {} kill -9 {} >/dev/null 2>&1 || true', (err) => { if(err) console.log('Port cleanup skipped:', err.message); }); }\"",
    "start": "cross-env FLASK_DEBUG=1 electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "eslint --ext .ts,.tsx .",
    "backend": "cross-env FLASK_DEBUG=1 python -m flask --app api/index run -p 5328",
    "build-flask": "cd api && python -m build"
  },
